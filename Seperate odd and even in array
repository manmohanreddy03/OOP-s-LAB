//print the most occuring elements
#include<iostream>
using namespace std;
int main()
{
	int i,k=0,j=0,ao[10],ae[10];
	int a[10]={1,2,3,4,5,6,7,8,9,12};
	
	for(i=0;i<10;i++)
	{
		if((a[i]%2)==0)
		{
			ae[k]=a[i];
			k++;
		}
		else
		{
			ao[j]=a[i];
			j++;
		}
	}
	cout<<"Even \n";
	for(i=0;i<k;i++)
	{
		cout<<ae[i]<<"\t";
	}
	cout<<"\nODD \n";
	for(i=0;i<j;i++)
	{
		cout<<ao[i]<<"\t";
	}
}
